\ProvidesPackage{psu-colors}
\RequirePackage{xcolor}

% All PSU colors are defined in terms of cmyk
% See https://www.pdx.edu/university-communications/tools-and-templates/brand-colors
\selectcolormodel{cmyk}

% Primary pallet
\definecolor{psu-green}{cmyk}{40,0,100,38}
\definecolor{psu-electric-green}{cmyk}{27,3,100,0}
\definecolor{psu-forest-green}{cmyk}{71,40,80,68}
\definecolor{psu-white}{cmyk}{0,0,0,0}
\definecolor{psu-black}{cmyk}{0,0,0,100}

% Secondary pallet
\definecolor{psu-red}{cmyk}{4,87,98,0}
\definecolor{psu-sand}{cmyk}{0,8,35,10}
\definecolor{psu-blue}{cmyk}{100,7,0,20}
\definecolor{psu-light-blue}{cmyk}{32,0,0,0}
\definecolor{psu-stone}{cmyk}{40,30,60,18}
\definecolor{psu-yellow}{cmyk}{0,25,100,0}
\definecolor{psu-orange}{cmyk}{0,55,100,0}
\definecolor{psu-purple}{cmyk}{48,100,0,40}
